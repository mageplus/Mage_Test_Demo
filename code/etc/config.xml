<config>
  <modules>
    <Foo_Bar>
      <version>0.0.1</version>
    </Foo_Bar>
  </modules>
  <global>
    <models>
      <catalog>
        <rewrite>
          <product>Foo_Bar_Model_Product</product>
        </rewrite>
      </catalog>
    </models>
  </global>

  <!-- this tells Mage_Test to include tests for this module -->
  <phpunit>
    <suite>
      <modules>
        <Foo_Bar />
      </modules>
    </suite>
  </phpunit>
</config>
